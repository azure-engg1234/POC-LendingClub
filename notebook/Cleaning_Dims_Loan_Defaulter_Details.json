				"source": [
					"df_loan_details = df_loan_details.withColumn('defaulter_id', split(df_loan_details['member_id'], 'I').getItem(1))\r\n",
					"df_loan_details = df_loan_details.withColumn(\"defaulter_id\", concat(lit(\"DEF\"), col(\"defaulter_id\")))"
