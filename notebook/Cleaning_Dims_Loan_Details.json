				"source": [
					"## Creating loan Id as surrogated key"
				]
			},
			{
				"cell_type": "code",
				"metadata": {
					"jupyter": {
						"source_hidden": false,
						"outputs_hidden": false
					},
					"nteract": {
						"transient": {
							"deleting": false
						}
					}
				},
				"source": [
					"df_loan_details = df_loan_details.withColumn('loan_id', split(df_loan_details['member_id'], 'I').getItem(1))\r\n",
					"df_loan_details = df_loan_details.withColumn(\"loan_id\",df_loan_details.loan_id.cast('int'))\r\n",
					"df_loan_details = df_loan_details.withColumn('loan_id', ( df_loan_details['loan_id'] - 999 ) )\r\n",
					"df_loan_details = df_loan_details.withColumn(\"loan_id\", concat(lit(\"LNI\"), col(\"loan_id\")))"
				],
				"execution_count": 3
			},
			{
				"cell_type": "markdown",
				"metadata": {
					"nteract": {
